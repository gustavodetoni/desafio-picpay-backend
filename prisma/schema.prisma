generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id       String  @id @default(uuid())
  name     String
  cpf      String? @unique
  cnpj     String? @unique
  email    String  @unique
  password String
  balance  Float
  type     String

  @@map("users")
}
